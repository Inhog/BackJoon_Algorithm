#include <iostream>
#include <algorithm>
#include <cstring>
using namespace std;

int N;
int A[1003], cache[1003];

int find(int a) {
	int& res = cache[a];

	if (res != -1) return res;
	res = 1;

	for (int j = a + 1; j < N; ++j) {
		if (A[a] > A[j]) { 
			res = max(res, find(j)+1); 
		}		
	}
	return res;
}


int main() {

	int rout = 0;

	memset(cache, -1, sizeof(cache));
	memset(A, -1, sizeof(A));
	cin >> N;

	for (int i = 0; i < N; ++i)
		cin >> A[i];

	for (int i = 0; i < N; ++i)
		rout = max(rout, find(i));

	cout << rout;

	return 0;
}
